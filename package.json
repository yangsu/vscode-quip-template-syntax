{
    "name": "quip-template",
    "displayName": "Quip Template",
    "description": "Quip Template Support for VSCode",
    "version": "0.0.1",
    "publisher": "yangsu",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "quip-template",
            "aliases": ["HTML (Quip Template)", "quip-template"],
            "extensions": [".html",".htm"],
            "configuration": "./quip-template-configuration.json"
        }],
        "grammars": [{
            "language": "quip-template",
            "scopeName": "text.html.quip-template",
            "path": "./syntaxes/quip-template.json"
        }],
        "snippets": [
            {
                "language": "quip-template",
                "path": "./snippets/quip-template.json"
            }
        ]
    }
}
